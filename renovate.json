{
  "extends": ["config:base"],
  "dependencyDashboard": false,
  "packageRules": [
    {
      "groupName": "python non-major dependencies",
      "groupSlug": "python-minor-patch",
      "matchLanguages": ["python"],
      "excludePackageNames": ["python"],
      "matchPackagePatterns": ["*"],
      "matchUpdateTypes": ["minor", "patch"]
    }
  ]
}
